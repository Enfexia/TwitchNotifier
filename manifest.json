{
  "name": "Twitch Notifier",
  "version": "0.1",
  "description": "",
  "author": "",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "webRequest",
    "notifications",
    "storage"
  ],
  "icons": {
    "64": "icon_enabled64.png"
  },
  "background": {
    "scripts": ["tmi.min.js","background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
        "64": "icon_enabled64.png"
      },
      "default_popup": "popup.html"
  }
}
